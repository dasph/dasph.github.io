  },
  "license": "MIT",
  "dependencies": {
    "@solidjs/router": "^0.10.10",
    "solid-js": "^1.8.17"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-solid": "^2.8.0"
  }
}
