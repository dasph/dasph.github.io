{
  "name": "homepage",
  "private": true,
  "version": "0.0.1",
  "description": "Andrew's personal homepage",
  "scripts": {
    "dev": "vite",
    "serve": "vite preview --outDir ./dist/client",
    "build:client": "vite build -m client",
